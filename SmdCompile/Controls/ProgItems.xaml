<UserControl x:Class="SmdCompile.Controls.ProgItems"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SmdCompile.Controls"
             mc:Ignorable="d" Background="{DynamicResource back_min_alpha_09}"
             >
    <Grid Margin="10">
        <StackPanel>
            <Label x:Name="NameProj_" Content="Name project" HorizontalAlignment="Left"></Label>
            <Label x:Name="Path_" Content="full local path" Margin="10,0,0,0" HorizontalAlignment="Left" Foreground="#FF969694"></Label>
        </StackPanel>        
    </Grid>
</UserControl>

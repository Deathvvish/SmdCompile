             xmlns:local="clr-namespace:SmdCompile"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="/FluentUI;component/Style/TabControl.xaml"/>
